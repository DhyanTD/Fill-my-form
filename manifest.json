{
  "manifest_version": 3,
  "name": "Form Field Extractor for AI",
  "version": "1.0",
  "description": "Extract form fields and prepare data for AI-powered auto-filling",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Extract Form Fields"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    /*    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png" */
  }
}
